PROGRAM KONVERSI_NILAI;{annur24.wordpress.com}
uses crt;
var nilaiAngka: integer;
begin
clrscr;
write('Masukkan nilai : ');
readln(nilaiAngka);
case (nilaiAngka) of
0..49 : write('Nilai E');
50..59 : write('Nilai D');
60..79 : write('Nilai C');
80..89 : write('Nilai B');
90..100 : write('Nilai A');
  else
  writeln('INPUT DATA SALAH');
end;
readkey;
end.
